Function main(a)
    Select Case a
        Case 1
            main = "PNAKC00291"
        Case 2
            main = "44#5#61#52"
        Case 3
            main = "猜想實驗裝置"
        Case 4
            main = "移動#3"
        Case 5
            main = "全距離#111"
        Case 6
            main = "特合計2以上#42a"
        Case 7
            main = "抽1張牌。&#本回合內自身技能造成的直接傷害+2"
    End Select
End Function

Function atking(a, VBEPersonVS(), VBEPageCardNumVS(), AtkingPagetotVS(), VBEPersonBuffVSF(), VBEPersonBuffVSS(), AtkingckVSS(), AtkingckVSF(), VBEAtkingVSF(), VBEAtkingVSS(), VBEActualStatusVS(), VBEVSStageNum())
    Select Case a
        Case 44
            If VBEAtkingVSS(12) = 2 Then
                If AtkingPagetotVS(1, 4) >= 2 Then
                    If AtkingckVSS(1, 1) = 0 Then
                        VBECommand.AtkingTurnOnOffWithLineLight 1
                    End If
                Else
                    If AtkingckVSS(1, 1) = 1 Then
                        VBECommand.AtkingTurnOnOffWithLineLight 2
                    End If
                End If
            End If
        Case 5
            VBECommand.AtkingStartPlay "\gif\No07\atking.jpg"
        Case 61
            VBECommand.AtkingDrawCards 1, 1, 1
        Case 52
            VBECommand.AtkingTurnOnOffWithLineLight 2
    End Select
    atking = VBECommand.Output()
End Function
