Function main(a)
    Select Case a
        Case 1
            main = "BUFFN00101"
        Case 2
            main = "45#53"
        Case 3
            main = "ATK+"
        Case 4
            main = "\Buff\atkup\atkup.png"
    End Select
End Function

Function buff(a, atkingpagetotVS(), VBEAtkingVSF(), VBEAtkingVSS(), VBEVSBuffNum(), BuffPersonType, VBEVSStageNum())
    Select Case a
        Case 45
            If BuffPersonType = 1 Then
                If VBEAtkingVSS(12) = 3 Then
                    VBECommand.EventPersonAbilityDiceChange 1, 1, VBEVSBuffNum(1)
                End If
            End If
        Case 53
            If BuffPersonType = 1 Then
                VBECommand.BuffTurnEnd
            End If
    End Select
    buff = VBECommand.Output()
End Function
